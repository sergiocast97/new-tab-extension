{
    "manifest_version": 3,
    "name": "New Tab.",
    "author": "sergiocast97",
    "version": "1.0",
    "description": "No frills, no nonsense new tab extension",
    "chrome_url_overrides": {
        "newtab": "index.html"
    },
    "permissions": ["storage"],
    "icons": {
        "16": "icons/icon-16.png",
        "24": "icons/icon-24.png",
        "32": "icons/icon-32.png",
        "48": "icons/icon-48.png",
        "128": "icons/icon-128.png"
      }
  }